<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.kok.mapper.RetrievedExperienceMapper">
<!--    insert-->
    <insert id="insertRetrievedExperience">
        insert into tbl_ai_retrieved_experience(member_id, experience_id)
        values (#{memberId}, #{experienceId})
    </insert>

<!--    select-->
    <select id="selectCountRetrieved">
        select count(*)
        from tbl_ai_retrieved_experience
        where member_id=#{memberId}
    </select>
</mapper>